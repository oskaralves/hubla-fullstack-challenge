model TransactionType {
    id           Int                   @id @default(autoincrement())
    description  String                @unique
    nature       TransactionNatureEnum
    transactions Transaction[]

    @@map("transaction_types")
    @@schema("public")
}

enum TransactionNatureEnum {
    INCOME // Entrada
    EXPENSE // Sa√≠da

    @@schema("public")
}
