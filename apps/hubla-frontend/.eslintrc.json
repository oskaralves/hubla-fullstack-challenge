{
  "env": {
    "commonjs": false,
    "es6": true
  },
  "extends": ["next/core-web-vitals", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "object-shorthand": ["error", "always"],
    "no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "none",
        "caughtErrors": "all",
        "ignoreRestSiblings": false
      }
    ]
  }
}
